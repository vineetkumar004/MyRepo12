<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://myuniversityfromvs.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>myuniversityfromvs.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>MyUniversityfromVS</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$MyUniversityfromVS</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="ContosoUniversity.DAL.SchoolContext" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:demoserveruniversity.database.windows.net,1433;Initial Catalog=demodatabase;User ID=vineet@demoserveruniversity;Password=Password@123" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="ContosoUniversity.DAL.SchoolContext, ContosoUniversity" MigrationConfiguration="ContosoUniversity.Migrations.Configuration, ContosoUniversity" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="MS_TableConnectionString" Order="2" Enabled="True">
          <Destination Path="Data Source=tcp:demoserveruniversity.database.windows.net,1433;Initial Catalog=demodatabase;User ID=vineet@demoserveruniversity;Password=Password@123" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=tcp:demoserveruniversity.database.windows.net,1433;Initial Catalog=demodatabase;User ID=vineet@demoserveruniversity;Password=Password@123" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\MS_TableConnectionString_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=tcp:demoserveruniversity.database.windows.net,1433;Initial Catalog=demodatabase;User ID=vineet@demoserveruniversity;Password=Password@123" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)ContosoUniversity.DAL.SchoolContext-Web.config Connection String">
      <ParameterValue>Data Source=tcp:demoserveruniversity.database.windows.net,1433;Initial Catalog=demodatabase;User ID=vineet@demoserveruniversity;Password=Password@123</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)MS_TableConnectionString-Web.config Connection String">
      <ParameterValue>Data Source=tcp:demoserveruniversity.database.windows.net,1433;Initial Catalog=demodatabase;User ID=vineet@demoserveruniversity;Password=Password@123</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="ContosoUniversity.DAL.SchoolContext" />
  </ItemGroup>
</Project>